#: lecture.adoc:408
msgid "== try-with-resources-блок"
msgstr ""

#: lecture.adoc:416
msgid ""
"Общая схема\n"
"[source,java]\n"
"----\n"
"try (Resource res = . . .) {\n"
"  work with res\n"
"}\n"
"----"
msgstr ""

#: lecture.adoc:426
msgid ""
"Пример:\n"
"[source,java]\n"
"----\n"
"try (Scanner in = new Scanner(\n"
"    new FileInputStream(\"/usr/share/dict/words\")), \"UTF-8\") {\n"
"  while (in.hasNext())\n"
"    System.out.println(in.next());\n"
"}\n"
"----"
msgstr ""

