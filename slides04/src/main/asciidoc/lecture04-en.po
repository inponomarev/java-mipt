"----"

#: lecture.adoc:388
msgctxt "lecture.adoc:388"
msgid "== Беда с finally-block"
msgstr "== A problem with `finally` block"

#: lecture.adoc:393
msgid ""
"[source,java]\n"
"----\n"
"...throws IOException...\n"
"//Два ресурса"
msgstr ""
"[source,java]\n"
"----\n"
"...throws IOException...\n"
"//Two resources"

#: lecture.adoc:404
msgid ""
"InputStream in = new FileInputStream(src);\n"
"try {\n"
"  OutputStream out = new FileOutputStream(dst);\n"
"  try {\n"
"    byte[] buf = new byte[BUFFER_SIZE];\n"
"    int n;\n"
"    while ((n = in.read(buf)) >= 0)out.write(buf, 0, n);} \n"
"  finally {out.close();}} \n"
"finally {in.close();}\n"
"----"
msgstr ""
"InputStream in = new FileInputStream(src);\n"
"try {\n"
"  OutputStream out = new FileOutputStream(dst);\n"
"  try {\n"
"    byte[] buf = new byte[BUFFER_SIZE];\n"
"    int n;\n"
"    while ((n = in.read(buf)) >= 0)out.write(buf, 0, n);} \n"
"  finally {out.close();}} \n"
"finally {in.close();}\n"
"----"

#: lecture.adoc:407
msgid "== try-with-resources-блок"
msgstr "== try-with-resources"

#: lecture.adoc:415
msgid ""
"Общая схема\n"
"[source,java]\n"
"----\n"
"try (Resource res = . . .) {\n"
"  work with res\n"
"}\n"
"----"
msgstr ""
"Common scheme\n"
"[source,java]\n"
"----\n"
"try (Resource res = . . .) {\n"
"  work with res\n"
"}\n"
"----"

#: lecture.adoc:425
msgid ""
"Пример:\n"
"[source,java]\n"
"----\n"
"try (Scanner in = new Scanner(\n"
"    new FileInputStream(\"/usr/share/dict/words\")), \"UTF-8\") {\n"
"  while (in.hasNext())\n"
"    System.out.println(in.next());\n"
"}\n"
"----"
msgstr ""
"Example:\n"
"[source,java]\n"
"----\n"
"try (Scanner in = new Scanner(\n"
"    new FileInputStream(\"/usr/share/dict/words\")), \"UTF-8\") {\n"
"  while (in.hasNext())\n"
"    System.out.println(in.next());\n"
"}\n"
"----"

#: lecture.adoc:427
msgid "== Множество ресурсов"
msgstr "== Multiple resources"

#: lecture.adoc:437
msgid ""
"[source,java]\n"
"----\n"
"try (Scanner in = new Scanner(\n"
"     new FileInputStream(\"/usr/share/dict/words\"), \"UTF-8\");\n"
"     PrintWriter out = new PrintWriter(\"out.txt\")) {\n"
"  while (in.hasNext())\n"
"    out.println(in.next().toUpperCase());\n"
"}\n"
"----"
msgstr ""
"[source,java]\n"
"----\n"
"try (Scanner in = new Scanner(\n"
"     new FileInputStream(\"/usr/share/dict/words\"), \"UTF-8\");\n"
"     PrintWriter out = new PrintWriter(\"out.txt\")) {\n"
"  while (in.hasNext())\n"
"    out.println(in.next().toUpperCase());\n"
"}\n"
"----"

