#: lecture.adoc:1142
msgid "== Сортируем по фамилии"
msgstr "== Sort by last name"

#: lecture.adoc:1165
msgid ""
"[cols=\"50a,50a\"]\n"
"|===\n"
"[source,java]\n"
"----\n"
"/*НЕПРАВИЛЬНО*/\n"
"Collections.sort(people,\n"
" (p1, p2) -> \n"
"   p1.getLastName()\n"
"   .compareTo(\n"
"     p2.getLastName());\n"
");\n"
"----\n"
"|\n"
"[source,java]\n"
"----\n"
"/*ПРАВИЛЬНО*/ \n"
"Collections.sort(people,\n"
"  Comparator.comparing\n"
"   (Person::getLastName));\n"
");\n"
"----\n"
"|==="
msgstr ""
"[cols=\"50a,50a\"]\n"
"|===\n"
"[source,java]\n"
"----\n"
"/*WRONG*/\n"
"Collections.sort(people,\n"
" (p1, p2) -> \n"
"   p1.getLastName()\n"
"   .compareTo(\n"
"     p2.getLastName());\n"
");\n"
"----\n"
"|\n"
"[source,java]\n"
"----\n"
"/*CORRECT*/ \n"
"Collections.sort(people,\n"
"  Comparator.comparing\n"
"   (Person::getLastName));\n"
");\n"
"----\n"
"|==="

#: lecture.adoc:1167
msgid "== {zsp}Сортируем по фамилии, потом по имени{zsp}"
msgstr "== Sort by last name, then by first name"

#: lecture.adoc:1181
msgid ""
"[source,java]\n"
"----\n"
"//НЕ ПИШИТЕ ВЕСЬ ЭТОТ КОШМАР\n"
"Collections.sort(people,\n"
"  (p1, p2) -> {\n"
"    int result = p1.getFirstName().compareTo(p2.getFirstName());\n"
"    if (result == 0) {\n"
"      result = p2.getLastName().compareTo(p2.getLastName());\n"
"    }\n"
"    return result;\n"
"  }\n"
");\n"
"----"
msgstr ""
"[source,java]\n"
"----\n"
"//DON'T WRITE THIS HORRIBLE MESS\n"
"Collections.sort(people,\n"
"  (p1, p2) -> {\n"
"    int result = p1.getFirstName().compareTo(p2.getFirstName());\n"
"    if (result == 0) {\n"
"      result = p2.getLastName().compareTo(p2.getLastName());\n"
"    }\n"
"    return result;\n"
"  }\n"
");\n"
"----"

#: lecture.adoc:1183
msgid "== Правильно так:"
msgstr "== Correct:"

#: lecture.adoc:1192
msgid ""
"[source,java]\n"
"----\n"
"Collections.sort(people,\n"
"  Comparator\n"
"    .comparing(Person::getLastName)\n"
"    .thenComparing(Person::getFirstName));\n"
");\n"
"----"
msgstr ""
"[source,java]\n"
"----\n"
"Collections.sort(people,\n"
"  Comparator\n"
"    .comparing(Person::getLastName)\n"
"    .thenComparing(Person::getFirstName));\n"
");\n"
"----"

