msgid "== Когда нужна memory model?"
msgstr "== When do we need a memory model?"

#: lecture.adoc:369
msgid ""
"[source,java]\n"
"----\n"
"class DumbWayToFallAsleep implements Runnable {\n"
"  private boolean asleep;"
msgstr ""
"[source,java]\n"
"----\n"
"class DumbWayToFallAsleep implements Runnable {\n"
"  private boolean asleep;"

#: lecture.adoc:373
msgctxt "lecture.adoc:373"
msgid ""
"  public void setAsleep(boolean asleep){\n"
"    this.asleep = asleep;\n"
"  }"
msgstr ""
"  public void setAsleep(boolean asleep){\n"
"    this.asleep = asleep;\n"
"  }"

#: lecture.adoc:383
msgid ""
"  @Override\n"
"  public void run() {\n"
"    while (!asleep){\n"
"      //countSomeSheep\n"
"      //УСНЁМ ЛИ МЫ?\n"
"    }\n"
"  }\n"
"}\n"
"----"
msgstr ""
"  @Override\n"
"  public void run() {\n"
"    while (!asleep){\n"
"      //countSomeSheep\n"
"      //WILL WE FALL ASLEEP?\n"
"    }\n"
"  }\n"
"}\n"
"----"

