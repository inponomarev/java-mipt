msgctxt "lecture.adoc:364"
msgid "== Когда нужна memory model?"
msgstr ""

#: lecture.adoc:369
msgid ""
"[source,java]\n"
"----\n"
"class DumbWayToFallAsleep implements Runnable {\n"
"  private boolean asleep;"
msgstr ""

#: lecture.adoc:373
msgctxt "lecture.adoc:373"
msgid ""
"  public void setAsleep(boolean asleep){\n"
"    this.asleep = asleep;\n"
"  }"
msgstr ""

#: lecture.adoc:383
msgid ""
"  @Override\n"
"  public void run() {\n"
"    while (!asleep){\n"
"      //countSomeSheep\n"
"      //УСНЁМ ЛИ МЫ?\n"
"    }\n"
"  }\n"
"}\n"
"----"
msgstr ""

#: lecture.adoc:385
msgctxt "lecture.adoc:385"
msgid "== Когда нужна memory model?"
msgstr ""

#: lecture.adoc:390
msgid ""
"[source,java]\n"
"----\n"
"class NotSoDumbWayToFallAsleep implements Runnable {\n"
"  private volatile boolean asleep;"
msgstr ""

#: lecture.adoc:394
msgctxt "lecture.adoc:394"
msgid ""
"  public void setAsleep(boolean asleep){\n"
"    this.asleep = asleep;\n"
"  }"
msgstr ""

#: lecture.adoc:404
msgid ""
"  @Override\n"
"  public void run() {\n"
"    while (!asleep){\n"
"      //countSomeSheep\n"
"      //...\n"
"    }\n"
"  }\n"
"}\n"
"----"
msgstr ""

