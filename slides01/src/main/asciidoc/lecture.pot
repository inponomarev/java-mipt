#: lecture.adoc:325
msgctxt "lecture.adoc:325"
msgid "== Когда нужна memory model?"
msgstr ""

#: lecture.adoc:330
msgid ""
"[source,java]\n"
"----\n"
"class DumbWayToFallAsleep implements Runnable {\n"
"  private boolean asleep;"
msgstr ""

#: lecture.adoc:334
msgctxt "lecture.adoc:334"
msgid ""
"  public void setAsleep(boolean asleep){\n"
"    this.asleep = asleep;\n"
"  }"
msgstr ""

#: lecture.adoc:344
msgid ""
"  @Override\n"
"  public void run() {\n"
"    while (!asleep){\n"
"      //countSomeSheep\n"
"      //УСНЁМ ЛИ МЫ?\n"
"    }\n"
"  }\n"
"}\n"
"----"
msgstr ""

#: lecture.adoc:346
msgctxt "lecture.adoc:346"
msgid "== Когда нужна memory model?"
msgstr ""

#: lecture.adoc:351
msgid ""
"[source,java]\n"
"----\n"
"class NotSoDumbWayToFallAsleep implements Runnable {\n"
"  private volatile boolean asleep;"
msgstr ""

#: lecture.adoc:355
msgctxt "lecture.adoc:355"
msgid ""
"  public void setAsleep(boolean asleep){\n"
"    this.asleep = asleep;\n"
"  }"
msgstr ""

#: lecture.adoc:365
msgid ""
"  @Override\n"
"  public void run() {\n"
"    while (!asleep){\n"
"      //countSomeSheep\n"
"      //...\n"
"    }\n"
"  }\n"
"}\n"
"----"
msgstr ""

